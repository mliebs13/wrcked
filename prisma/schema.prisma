generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Admin {
  id        String   @id() @default(uuid())
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt

  username      String @db.VarChar(255)
  password      String @db.VarChar(255)
}